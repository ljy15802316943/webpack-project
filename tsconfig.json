{
  "compilerOptions": {
    "outDir": "disttest",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    //路径别名配置
    "paths": {
      "@pages/*": ["src/pages/*"],
      "@assets/*": ["src/assets/*"],
    }
  }
}